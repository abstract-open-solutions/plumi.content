<dl class="portlet portletPublish"
    i18n:domain="plumi.portlets"
    tal:define="portal_url context/@@plone_portal_state/portal_url;
                portal context/@@plone_portal_state/portal;">


    <dt class="portletHeader publishPortletHeader">
        <span class="portletTopLeft"></span>
        Publish
        <span class="portletTopRight"></span>
    </dt>

    <dd class="portletItem publish-portlet">
        <dl>
            <ul>
                <li>
                    <a class="optionHeader" tal:attributes="href string:${portal/portal_membership/getHomeUrl}/videos/@@publish_video" i18n:domain="plumi" i18n:translate="">Publish Video</a>
                <li>

                <li>
                    <a class="optionHeader" tal:attributes="href string:${portal/portal_membership/getHomeUrl}/events/createObject?type_name=Event" i18n:domain="plumi" i18n:translate="">Publish Event</a>
                <li>

                <li>
                    <a class="optionHeader" tal:attributes="href string:${portal/portal_membership/getHomeUrl}/news/createObject?type_name=News Item" i18n:domain="plumi" i18n:translate="">Publish News</a>
                </li>
            </ul>
        </dl>
    </dd>

    <dd class="portletFooter">
        <span class="portletBottomLeft"></span>
        <span class="portletBottomRight"></span>
    </dd>

</dl>
